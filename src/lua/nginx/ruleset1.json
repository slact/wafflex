{
  //is this allowed?
  "name": "ruleset1",
  "table": {
    "request": [ "list1" ]
  },
  "lists": {
    "list1": [
      "fweep",
      "fwoop",
      {
        "info": "This rule rate-limits by ip",
        "if": {"true" : {
          "name": "rate-limit",
          "key": "$request_real_ip"
        }},
        "then": [
          {"#tag": "slowdown"},
          {"#reject": {"status": 403, "body": "Slow down!!!"}}
        ]
      }
    ]
  },
  "rules": {   //named rules
    "fweep": {
      "if": { "#tag-check": "foo"},
      "then": {"#tag":"yeah"}
    }, 
    "fwoop": { "always": { "#tag":"this is a tag yo" } },
    "rule1": { 
      "if-all": ["#true", {"any": ["#true", "#false"]}],
      "then": "accept",
      "else": "reject"
    }
  }
}
